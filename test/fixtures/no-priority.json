{
    "logger": {
        "enabled": true,
        "priority": 1,
        "module": {
            "name": "koa-morgan",
            "arguments": [ "combined" ]
        }
    },

    "favicon": {
        "enabled": true,
        "module": {
            "name": "koa-favicon",
