{
  "name": "koa-meddleware",
  "version": "2.0.0",
  "description": "Configuration-based middleware registration for koa.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tape test/*.js",
    "cover": "istanbul cover tape -- test/*.js",
    "lint": "jshint -c .jshintrc index.js",
    "dryrun": "(TARBALL=`npm pack` ; tar ztvf $TARBALL ; rm -f $TARBALL)"
  },
  "keywords": [
    "koa",
    "middleware"
  ],
  "author": [
    "Erik Toth <ertoth@paypal.com>",
    "Martin Denyer <martin.denyer@engagecrm.co.uk>"
  ],
  "licenses": [
    {
      "type": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  ],
  "devDependencies": {
    "istanbul": "^0.2.7",
    "jshint": "^2.5.0",
    "koa": "^2.3.0",
    "koa-body": "^2.3.0",
    "koa-cookie": "^1.0.0",
    "koa-favicon": "^2.0.0",
    "koa-morgan": "^1.0.1",
    "koa-send": "^4.1.0",
    "koa-static": "^3.0.0",
    "shortstop": "^1.0.1",
    "shortstop-handlers": "^1.0.0",
    "shortstop-regex": "0.0.1",
    "supertest": "^0.11.0",
    "tape": "^2.12.3"
  },
  "dependencies": {
    "any-promise": "^1.3.0",
    "caller": "0.0.1",
    "core-util-is": "^1.0.2",
    "debuglog": "^1.0.1",
    "koa-mount": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kaleidoc0de/meddleware.git"
  },
  "bugs": {
    "url": "https://github.com/krakenjs/meddleware/issues"
  },
  "homepage": "https://github.com/krakenjs/meddleware"
}
